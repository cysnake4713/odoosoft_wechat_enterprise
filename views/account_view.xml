<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_wechat_enterprise_account_tree" model="ir.ui.view">
            <field name="name">view.odoosoft.wechat.enterprise.account.tree</field>
            <field name="model">odoosoft.wechat.enterprise.account</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="corp_id"/>
                </tree>
            </field>
        </record>

        <record id="view_wechat_enterprise_account_form" model="ir.ui.view">
            <field name="name">view.odoosoft.wechat.enterprise.account.form</field>
            <field name="model">odoosoft.wechat.enterprise.account</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="code"/>
                            <field name="name"/>
                            <field name="corp_id"/>
                            <field name="corpsecret" groups="odoosoft_wechat_enterprise.group_wechat_enterprise_admin"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>